{
    "name": "rossmitchell/xfi-currency",
    "description": "Currency for use with MoneyPHP that handle Financial Prices with fractions of a penny",
    "require": {
        "php": ">=7.1",
        "moneyphp/money": "^3.2"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "RossMitchell\\XfiCurrency\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RossMitchell\\XfiCurrency\\Tests\\": "tests/"
        }
    }
}
